{
    "id": "{user:user.id?my}-mtt-mcc",
    "plan": {
        "type": "local",
        "path": "{_source_:mtt_common}/terraform/aws"
    },
    "state": {
        "path": "{_source_:project}/terraform/{id}.terraform.state"
    },
    "options": {
        "always_init": true
    },
    "vars_path": "{_source_:project}/terraform/{id}.tfvars.json",
    "vars": {
        "cluster_name": "{id}",
        "master_count": 1,
        "worker_count": 3,
        "windows_worker_count": 0,
        "aws_region": "us-east-2"
    }
}
